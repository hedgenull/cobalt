# Compute the Fibonacci Sequence

Module {
    Var a = 1;
    Var b = 1;

    PrintLn("Fibonacci Sequence");
    Var NumberOfTimes = 10;

    While NumberOfTimes > 0 {
        PrintLn(a + b);
        Var c = a;
        Var a = b;
        Var b = b + c;
        Var NumberOfTimes = NumberOfTimes - 1;
    };
};